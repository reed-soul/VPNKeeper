{
  "manifest_version": 3,
  "name": "VPN Keeper",
  "version": "1.0",
  "description": "保持VPN连接的Chrome插件",
  "permissions": [
    "alarms",
    "storage"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  }
} 